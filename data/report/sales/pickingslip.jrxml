                <!-- Zip Code + City -->
                <textField isBlankWhenNull="true">
                    <reportElement x="10" y="56" width="240" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.invoiceadress.zipcode} + " " + $P{order.invoiceadress.city}]]>
                    </textFieldExpression>
                </textField>

                <!-- Country -->
                <textField isBlankWhenNull="true">
                    <reportElement x="10" y="70" width="240" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.invoiceadress.country}]]>
                    </textFieldExpression>
                </textField>

            </frame>

            <!-- Order details, Column 1 -->
            <frame>
                <reportElement x="45" y="90" width="250" height="76"/>

                <!-- Our contact -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="0" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.ourcontact}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="0" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.ourcontact}]]>
                    </textFieldExpression>
                </textField>


                <!-- Deliveryterm -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="15" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.deliveryterm}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="15" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.deliveryterm}]]>
                    </textFieldExpression>
                </textField>

                <!-- Delivery way -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="30" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.deliveryway}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="30" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.deliveryway}]]>
                    </textFieldExpression>
                </textField>

                <!-- Payment term -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="45" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.paymentterm}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="45" width="150" height="12"/>
                    <textElement textAlignment="Left">
