                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.delayinterest}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="60" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.delayinterest} != null ? $P{order.delayinterest} + "%" : null]]>
                    </textFieldExpression>
                </textField>



            </frame>

            <!-- Order details, Column 2 -->
            <frame>
                <reportElement x="305" y="90" width="250" height="76"/>

                <!-- Customer nr -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="0" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.customernr}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="0" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.customernr}]]>
                    </textFieldExpression>
                </textField>


                <!-- Your contact -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="15" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.yourcontact}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="15" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.yourcontact}]]>
                    </textFieldExpression>
                </textField>

                <!-- Your order number -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="30" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.yourordernumber}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="30" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.yourordernumber}]]>
                    </textFieldExpression>
                </textField>

                <!-- Estimated delivery -->
                <textField isBlankWhenNull="false">
                    <reportElement x="0" y="45" width="100" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.estimateddelivery}]]>
                    </textFieldExpression>
                </textField>

                <textField isBlankWhenNull="true">
                    <reportElement x="100" y="45" width="150" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="8" />
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.estimateddelivery}]]>
                    </textFieldExpression>
                </textField>


            </frame>

        </band>
    </columnHeader>


    <detail>
        <band isSplitAllowed="true" height="436">
            <!-- Subreport for the rows 385-->
            <frame>
                <reportElement x="40" y="0" width="510" height="411" isPrintWhenDetailOverflows="true"/>

                <box border="Thin" borderColor="#000000"  topBorder="None" />

                <subreport isUsingCache="false">
                    <reportElement x="0" y="0" width="510" height="411" isPrintWhenDetailOverflows="true"/>

                    <parametersMapExpression>
                        <![CDATA[$P{REPORT_PARAMETERS_MAP}]]>
                    </parametersMapExpression>

                    <subreportParameter name="REPORT_RESOURCE_BUNDLE">
                        <subreportParameterExpression>
                            <![CDATA[$P{REPORT_RESOURCE_BUNDLE}]]>
                        </subreportParameterExpression>
                    </subreportParameter>

                    <dataSourceExpression>
                        <![CDATA[$F{subreport.datasource}]]>
                    </dataSourceExpression>

                    <returnValue subreportVariable="totalweight"      toVariable="totalweight"/>
                    <returnValue subreportVariable="totalvolume"      toVariable="totalvolume"/>

                    <subreportExpression class="net.sf.jasperreports.engine.JasperReport">
                        <![CDATA[$P{subreport.report}]]>
                    </subreportExpression>


                </subreport>
            </frame>

            <!-- Summary fields -->
            <frame>
                <reportElement x="40" y="411" width="510" height="25" positionType="Float"/>

                <box border="Thin" borderColor="#000000" topBorder="None" />


                <line>
                    <reportElement x="290" y="0" width="1" height="25"/>
                    <graphicElement pen="Thin"/>
                </line>


                <!-- Total weight-->
                <textField>
                    <reportElement x="293" y="1" width="77" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="7"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.totalweight}]]>
                    </textFieldExpression>
                </textField>

                <textField pattern="#,##0.000" isBlankWhenNull="true" evaluationTime="Report">
                    <reportElement x="290" y="10" width="77" height="14"/>
                    <textElement textAlignment="Right">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression class="java.math.BigDecimal">
                        <![CDATA[$V{totalweight}]]>
                    </textFieldExpression>
                </textField>

                <textField>
                    <reportElement x="370" y="10" width="29" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{company.weightunit}]]>
                    </textFieldExpression>
                </textField>


                <line>
                    <reportElement x="400" y="0" width="1" height="25"/>
                    <graphicElement pen="Thin"/>
                </line>

                <!-- Total volume-->
                <textField>
                    <reportElement x="403" y="1" width="77" height="12"/>
                    <textElement textAlignment="Left">
                        <font size="7"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.totalvolume}]]>
                    </textFieldExpression>
                </textField>

                <textField pattern="#,##0.000" isBlankWhenNull="true" evaluationTime="Report">
                    <reportElement x="400" y="10" width="77" height="14"/>
                    <textElement textAlignment="Right">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression class="java.math.BigDecimal">
                        <![CDATA[$V{totalvolume}]]>
                    </textFieldExpression>
                </textField>

                <textField>
                    <reportElement x="480" y="10" width="30" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{company.volumeunit}]]>
                    </textFieldExpression>
                </textField>





            </frame>


        </band>
    </detail>



</jasperReport>
