                <textField isBlankWhenNull="true">
                    <reportElement x="5" y="2" width="240" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="8"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$R{pickingslipreport.invoiceadress}]]>
                    </textFieldExpression>
                </textField>

                <!-- Name -->
                <textField isBlankWhenNull="true">
                    <reportElement x="10" y="14" width="240" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.invoiceadress.name}]]>
                    </textFieldExpression>
                </textField>

                <!-- Address 1 -->
                <textField isBlankWhenNull="true">
                    <reportElement x="10" y="28" width="240" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.invoiceadress.address1}]]>
                    </textFieldExpression>
                </textField>

                <!-- Address 2 -->
                <textField isBlankWhenNull="true">
                    <reportElement x="10" y="42" width="240" height="14"/>
                    <textElement textAlignment="Left">
                        <font size="10"/>
                    </textElement>
                    <textFieldExpression>
                        <![CDATA[$P{order.invoiceadress.address2}]]>
                    </textFieldExpression>
